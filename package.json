{
    "name": "shiba-syntax-highlight",
    "displayName": "Shiba syntax highlight",
    "version": "0.0.1",
    "publisher": "shiba",
    "description": "Syntax highlight for Shiba lang",
    "author": {"name": "ldakhoa"},
    "categories": [
        "Programming Languages"
    ],
    "engines": {
        "vscode": "^1.51.0"
    },
    "contributes": {
        "languages": [{
            "id": "shiba",
            "aliases": ["shiba", "shiba"],
            "extensions": [".shiba"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "shiba",
            "scopeName": "source.shiba",
            "path": "./syntaxes/shiba.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": ""
    }
}